{
  "name": "autoworks-player",
  "version": "0.2.1",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "build:library:npm": "webpack --config ./config/webpack/webpack.lib.js",
    "build:library:dts": "cat src/component/Structure/Player/index.jsx | react2dts --module-name autoworks-player > dist/AutoWorksPlayer.d.ts",
    "build:library:browser": "webpack --config ./config/webpack/webpack.browser.js",
    "build:library:standalone": "webpack --config ./config/webpack/webpack.standalone.js",
    "start": "next start",
    "export:storybook": "yarn build-storybook -s ./public && echo 'User-agent: *\nDisallow: /' > storybook-static/robots.txt",
    "export:app": "next export",
    "export:library": "npm-run-all --parallel build:library:*",
    "export": "npm-run-all build export:**",
    "test": "run-p qa:**",
    "qa:test": "jest",
    "qa:js": "eslint . --cache --ext '.js,.jsx'",
    "qa:css": "stylelint \"src/**/*.scss\"",
    "prettier": "prettier --write '**/*.{js,jsx,css,scss}'",
    "test:dev": "jest --coverage --watch --verbose",
    "coverage": "jest --coverage",
    "analyze": "ANALYZE=true yarn build",
    "storybook": "start-storybook -s ./public -p 3341",
    "build-storybook": "build-storybook -s ./public"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/AutoWorksPlayer.js",
  "types": "dist/AutoWorksPlayer.d.ts",
  "author": "",
  "license": "ISC",
